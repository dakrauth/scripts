# Pretty print various common things
alias ppd pp dir(%1)
alias ppg pp globals()
alias ppl pp locals()
alias ppo pp %1.__dict__
alias pps ppo self
alias ppv pp vars(%1)

# Next and list
alias nl n;;l

# Step and list
alias sl s;;l

alias inspect import inspect;print inspect.getsource(%1)
alias tb tbreak %1;;c
